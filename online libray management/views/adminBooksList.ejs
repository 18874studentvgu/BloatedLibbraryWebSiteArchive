<!DOCTYPE html>
<html lang="english">
  <%- include('layouts/header'); -%>
  <body>
    <div>
      <link href="./css/admin-book-board.css" rel="stylesheet" />
      <link href="https://cdn.datatables.net/1.13.4/css/jquery.dataTables.min.css" rel="stylesheet" />
      <div class="admin-book-board-container">
        <div class="admin-book-board-container01">
          <nav id="MainNav" class="admin-book-board-nav-bar">
            <div class="admin-book-board-container02">
              <a href="/index" class="admin-book-board-navlink">
                <img
                  alt="VGULogo11954"
                  src="playground_assets/vgulogo11954-887-200h.png"
                  class="admin-book-board-vg-logo1"
                />
              </a>
              <div class="admin-book-board-navbutton">
                <a href="/index" class="admin-book-board-navlink1">
                  <h2 class="admin-book-board-text">
                    <span>Home</span>
                    <span></span>
                  </h2>
                </a>
                <a href="/contact" class="admin-book-board-navlink2">
                  <h2 class="admin-book-board-text003"><span>Contact</span></h2>
                </a>
                <a href="/about" class="admin-book-board-navlink3">
                  <h2 class="admin-book-board-text005">About</h2>
                </a>
              </div>
              <a
                href="search-filter-book.html"
                class="admin-book-board-navlink4"
              >
                <img
                  alt="magnifyingglass11955"
                  src="playground_assets/magnifyingglass11955-xoij-200h.png"
                  class="admin-book-board-magnifyingglass1"
                />
              </a>
              <a
                href="/login"
                class="admin-book-board-add-new-book-button button"
              >
                Hi, User name
              </a>
            </div>
          </nav>
          <%- include('layouts/adminheader'); -%>
          <div class="admin-book-board-searchbar">
            <button
              id="add new user"
              type="submit"
              class="admin-book-board-add-new-user-button button"
            >
              Add new Book
            </button>
          </div>
          <div class="admin-book-board-adminuserboard">
            <div class="admin-book-board-container03">
              <div class="admin-book-board-newusertable">
                <table style="width: 100%; height: 100%;" id = "table">
                  <thead>
                    <tr>
                      <th class="admin-book-board-text006">PFP</th>
                      <th class="admin-book-board-text006">ID</th>
                      <th class="admin-book-board-text008">Name</th>
                      <th class="admin-book-board-text009" style="width: 10%;">Author</th>
                      <th class="admin-book-board-text010">Cat.</th>
                      <th class="admin-book-board-text011">Pub. date</th>
                      <th class="admin-book-board-text012">Up. date</th>
                      <th class="admin-book-board-text013">Quan.</th>
                      <th class="admin-book-board-text014">Rec. %</th>
                      <th class="admin-book-board-text014">Edit</th>
                      <th class="admin-book-board-text014">Remove</th>
                      <th class="admin-book-board-text014">Info</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% for (var i = 0; i < booksList.length; i++) { %>
                    <tr>
                      <td>
                        <img
                          alt="image"
                          src="playground_assets/user112951-5rs-200h.png"
                          class="admin-book-board-image"
                        />
                      </td>
                      <td class="admin-book-board-text015">123</td>
                      <td class="admin-book-board-text016"><%= booksList[i].Title %></td>
                      <td class="admin-book-board-text017"><%= booksList[i].Author %></td>
                      <td class="admin-book-board-text018"><%= booksList[i].Category %></td>
                      <td class="admin-book-board-text019"><%= booksList[i].publishedDate %></td>
                      <td class="admin-book-board-text020"><%= booksList[i].uploadedDate.toDateString() %></td>
                      <td class="admin-book-board-text023"><%= booksList[i].Quantity %></td>
                      <td class="admin-book-board-text026"><%= booksList[i].Recommendation %>%</td>
                      <br/>
                      <td>
                        <img
                          alt="draw13434"
                          src="playground_assets/draw13434-8jw-200w.png"
                          class="admin-book-board-draw1"
                        />
                      </td>
                      <td>
                        <img
                          alt="trashcan13434"
                          src="playground_assets/trashcan13434-meoe-200w.png"
                          class="admin-book-board-trashcan1"
                        />
                      </td>
                      <td>
                        <img
                          alt="image"
                          src="playground_assets/trashcan13831-is4-200w.png"
                          class="admin-book-board-image01"
                        />
                      </td>
                    </tr>
                    <% } %>
                  </tbody>
                </table>
              </div>
            </div>
            <!-- <div class="admin-book-board-rectangle50">
              <img
                alt="Rectangle513521"
                src="playground_assets/rectangle513521-a7pb-200w.png"
                class="admin-book-board-rectangle51"
              />
            </div> -->
          </div>
        </div>
        <!--footer-->
        <%- include('layouts/footer'); -%>
      </div>
    </div>
  </body>
  <!--Script-->
  <%- include('layouts/script'); -%>
  <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
  <script src="https://cdn.datatables.net/1.13.4/js/jquery.dataTables.min.js"></script>
  <script src="js/tablesorting.js" type="text/javascript"></script>
</html>
