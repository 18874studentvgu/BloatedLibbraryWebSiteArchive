<!DOCTYPE html>
<html lang="english">
    <%- include('layouts/header'); -%>
    <body>
        <div>
            <link href="/css/admin-add-book.css" rel="stylesheet" />
            <div class="add-book-container">
                <div class="add-book-header">
                    <%- include('layouts/navbar'); -%>
                    <%- include('layouts/adminheader'); -%>
                </div>
                <div class="add-book-container2">
                    <div class="add-book-login">
                        <div class="add-book-background">
                            <h1 class="add-book-text06">Add book</h1>
                            <form action="/users/login" method="POST" class="add-book-form">
                                <div class="add-book-book-name">
                                    <label for="username" class="add-book-text07">
                                        Book name
                                    </label>
                                    <input
                                        type="text"
                                        id="username"
                                        class="add-book-textinput input"
                                    />
                                </div>
                                <div class="add-book-author">
                                    <label for="password" class="add-book-text08">
                                        Author (separated by commas)
                                    </label>
                                    <input
                                        type="password"
                                        id="password"
                                        class="add-book-textinput1 input"
                                    />
                                </div>
                                <div class="add-book-author1">
                                    <label for="password" class="add-book-text09">
                                        Category (separated by commas)
                                    </label>
                                    <input
                                        type="password"
                                        id="password"
                                        class="add-book-textinput2 input"
                                    />
                                </div>
                                <div class="add-book-edition">
                                    <label for="password" class="add-book-text10">Edition</label>
                                    <input
                                        type="password"
                                        id="password"
                                        class="add-book-textinput3 input"
                                    />
                                </div>
                                <div class="add-book-pub-year">
                                    <label for="password" class="add-book-text11">
                                        Published year
                                    </label>
                                    <input
                                        type="password"
                                        id="password"
                                        class="add-book-textinput4 input"
                                    />
                                </div>
                                <div class="add-book-quantity">
                                    <label for="password" class="add-book-text12">
                                        No. of books
                                    </label>
                                    <input
                                        type="password"
                                        id="password"
                                        class="add-book-textinput5 input"
                                    />
                                </div>
                                <a href="index.html" class="add-book-login1 button">Login</a>
                            </form>
                            <div class="add-book-input-info"></div>
                        </div>
                    </div>
                </div>
                <%- include('layouts/footer'); -%>
            </div>
        </div>
    </body>
</html>
