<!DOCTYPE html>
<html lang="english">
    <%- include('layouts/header'); -%>
  <body>
    <div>
      <link href="/css/admin-add-user.css" rel="stylesheet" />

      <div class="add-user-container">
        <div class="add-user-header">
            <%- include('layouts/navbar'); -%>
            <%- include('layouts/adminheader'); -%>
        </div>
        <div class="add-user-container2">
          <div class="add-user-login">
            <div class="add-user-background">
              <h1 class="add-user-text06">Add user</h1>
              <form action="/users/login" method="POST" class="add-user-form">
                <div class="add-user-full-name">
                  <label for="username" class="add-user-text07">
                    Full name
                  </label>
                  <input
                    type="text"
                    id="username"
                    class="add-user-textinput input"
                  />
                </div>
                <div class="add-user-username">
                  <label for="password" class="add-user-text08">Username</label>
                  <input
                    type="password"
                    id="password"
                    class="add-user-textinput1 input"
                  />
                </div>
                <div class="add-user-edition">
                  <label for="password" class="add-user-text09">Age</label>
                  <input
                    type="password"
                    id="password"
                    class="add-user-textinput2 input"
                  />
                </div>
                <div class="add-user-pub-year">
                  <label for="password" class="add-user-text10">Email</label>
                  <input
                    type="password"
                    id="password"
                    class="add-user-textinput3 input"
                  />
                </div>
                <a href="index.html" class="add-user-login1 button">Login</a>
              </form>
              <div class="add-user-input-info"></div>
            </div>
          </div>
        </div>
        <%- include('layouts/footer'); -%>
      </div>
    </div>
  </body>
</html>
